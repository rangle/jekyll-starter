{
  "name": "jekyll-starter",
  "version": "0.0.1",
  "description": "A Gulp based Jekyll starter",
  "license": "MIT",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run gulp build",
    "clean": "rimraf _site && rimraf dist",
    "dev": "NODE_ENV=development && npm run gulp dev",
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "lint": "eslint .",
    "preinstall": "bundle install",
    "start": "npm run dev",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/jekyll-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/jekyll-starter"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "^6.6.0",
    "basscss": "^8.0.0",
    "browser-sync": "^2.12.5",
    "del": "^2.2.0",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^5.0.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-cssnano": "^2.1.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-util": "^3.0.7",
    "postcss-cssnext": "^2.5.2",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^8.1.0",
    "postcss-nested": "^1.0.0",
    "postcss-strip-inline-comments": "^0.1.3",
    "rimraf": "^2.4.3",
    "yargs": "^4.4.0"
  }
